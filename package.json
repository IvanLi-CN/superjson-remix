{
  "version": "0.3.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup --clean",
    "test": "tsc --noEmit ./test/type.test.ts",
    "size": "size-limit",
    "prepack": "pnpm run build",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@IvanLi-CN/superjson-remix",
  "author": {
    "name": "Ivan Li",
    "url": "https://github.com/IvanLi-CN"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@remix-run/dev": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "@remix-run/serve": "^2.3.1",
    "@size-limit/preset-small-lib": "^11.0.1",
    "@types/node": "^20",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^11.0.1",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "superjson": "^2.2.1"
  },
  "peerDependencies": {
    "@remix-run/node": ">=2.0.1",
    "@remix-run/react": ">=2.0.1"
  }
}
